<?xml version="1.0" encoding="utf-8"?>
<Resources>
	<CustomTextureList>
		<CustomTexture>
			<File>{filename}</File>
			<Projection>Triangle</Projection>
			<BottomLeft>
				<Longitude>{LLHC_lon}</Longitude>
				<Latitude>{LLHC_lat}</Latitude>
			</BottomLeft>
			<BottomRight>
				<Longitude>{LRHC_lon}</Longitude>
				<Latitude>{LRHC_lat}</Latitude>
			</BottomRight>
			<TopRight>
				<Longitude>{URHC_lon}</Longitude>
				<Latitude>{URHC_lat}</Latitude>
			</TopRight>
			<TopLeft>
				<Longitude>{ULHC_lon}</Longitude>
				<Latitude>{ULHC_lat}</Latitude>
			</TopLeft>
		</CustomTexture>
	</CustomTextureList>
</Resources>